<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".QuickSelect" android:id="@+id/quickselectlayout">

    <android.support.v7.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            tools:layout_editor_absoluteX="0dp" android:id="@+id/toolbar2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"

            android:layout_height="wrap_content"
            android:id="@+id/mediabutton1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbar2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton2"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton2" app:layout_constraintStart_toEndOf="@+id/mediabutton1"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/toolbar2"
            app:layout_constraintEnd_toStartOf="@+id/mediabutton3" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton3" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbar2" app:layout_constraintStart_toEndOf="@+id/mediabutton2"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton1" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton5"
            android:layout_marginEnd="8dp"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/mediabutton5"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton2"
            app:layout_constraintStart_toEndOf="@+id/mediabutton4" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/mediabutton6" android:layout_marginEnd="90dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton3" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/mediabutton5"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.711"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton7" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/mediabutton4"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutto8" app:layout_constraintStart_toEndOf="@+id/mediabutton7"
            android:layout_marginStart="64dp" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton5"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mediabutton9" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton6"
            app:layout_constraintStart_toEndOf="@+id/mediabutto8" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
</android.support.constraint.ConstraintLayout>