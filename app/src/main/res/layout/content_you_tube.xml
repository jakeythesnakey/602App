<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_you_tube"
        tools:context=".YouTubeActivity" android:visibility="visible"
        android:background="#6973C4">

    <EditText
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/minutes"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:text="0"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/address"/>
    <EditText
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/seconds"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:text="0" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/address"/>
    <EditText
            android:layout_width="394dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="YouTube Video Address"
            android:ems="10"
            android:id="@+id/address"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Minutes"
            android:layout_width="190dp"
            android:layout_height="41dp"
            android:id="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/minutes"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Seconds"
            android:layout_width="190dp"
            android:layout_height="41dp"
            android:id="@+id/textView3" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/seconds"/>
    <Button
            android:text="Launch Video"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonLaunch" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>

    <Button
            android:background="@drawable/smiling"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton4"
            android:id="@+id/mediabutton1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/buttonLaunch" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton2"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/veryangry"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton5"
            android:id="@+id/mediabutton2" app:layout_constraintStart_toEndOf="@+id/mediabutton1"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/buttonLaunch"
            app:layout_constraintEnd_toStartOf="@+id/mediabutton3" android:layout_marginEnd="8dp"/>
    <Button
            android:background="@drawable/cryingface"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton6"
            android:id="@+id/mediabutton3" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/buttonLaunch" app:layout_constraintStart_toEndOf="@+id/mediabutton2"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>

    <Button
            android:background="@drawable/poop"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton7"
            android:id="@+id/mediabutton4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton1" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton5"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/middlefinger"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton8"
            android:id="@+id/mediabutton5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton2"
            app:layout_constraintStart_toEndOf="@+id/mediabutton4"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton6"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/sleeping"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/mediabutton9"
            android:id="@+id/mediabutton6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton3"
            app:layout_constraintStart_toEndOf="@+id/mediabutton5"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/smilingdevil"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/mediabutton7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton8"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/fisthand"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/mediabutton8" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton5"
            app:layout_constraintStart_toEndOf="@+id/mediabutton7"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/mediabutton9"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

    <Button
            android:background="@drawable/horns"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/mediabutton9" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mediabutton6"
            app:layout_constraintStart_toEndOf="@+id/mediabutton8"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.6"/>

</android.support.constraint.ConstraintLayout>